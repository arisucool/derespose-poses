{
  "name": "derespose-poses",
  "version": "1.0.0",
  "description": "This repository stores pose datas used by Derespose",
  "scripts": {
    "start": "npm run dist",
    "test": "jest",
    "dist": "rimraf dist && mkdir -p dist && npm run dist:docs && npm run dist:poses",
    "dist:docs": "marked -o dist/index.html -i README.md",
    "dist:poses": "cp poses/poses.json dist/ && ts-node src/dist-pose-generator.ts"
  },
  "dependencies": {
    "jimp": "^0.16.2",
    "jszip": "^3.10.1",
    "marked": "^4.2.5",
    "ngx-mp-pose-extractor": "github:mugiply/mp-video-pose-extractor#library",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "jest": "^29.4.1",
    "jest-junit": "^15.0.0",
    "ts-jest": "^29.0.5"
  },
  "jest": {
    "preset": "ts-jest",
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".spec.ts$"
  },
  "jest-junit": {
    "outputName": "jest-junit.xml",
    "ancestorSeparator": " â€º ",
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  }
}
